@model IEnumerable<NauAnUtLanh.Database.Category>
<style>
    .menu-icon {
        height: 32px;
        width: 32px;
        margin-right: 10px;
    }
</style>

<div class="reservation-left">
    <h4>Loại món ăn</h4>
    <ul>
        @{
            foreach (var cate in Model)
            {
                <li><a href="@Url.Action("index", "food", new {type = cate.Id})">
                    <img src="@Url.Content("~/images/logo-menu.png")" alt="" class="category-icon"/> @cate.CategoryName
                </a></li>
            }
        }
    </ul>
</div>
